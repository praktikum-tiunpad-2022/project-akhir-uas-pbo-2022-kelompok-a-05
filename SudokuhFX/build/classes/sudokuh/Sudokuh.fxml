<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="sudokuh.SudokuhController">
    <stylesheets>
        <URL value="@sudokuh.css" />
    </stylesheets>
   <children>
      <MenuBar layoutY="1.0" prefHeight="26.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="About">
            <items>
              <MenuItem mnemonicParsing="false" text="Sudoku Game" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="How To Play" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Menu">
            <items>
                  <Menu mnemonicParsing="false" text="New">
                     <items>
                        <Menu mnemonicParsing="false" text="Level Game">
                           <items>
                              <MenuItem mnemonicParsing="false" text="Easy" />
                              <MenuItem mnemonicParsing="false" text="Medium" />
                              <MenuItem mnemonicParsing="false" text="Hard" />
                           </items>
                        </Menu>
                     </items>
                  </Menu>
              <MenuItem mnemonicParsing="false" text="Restart Game" />
                  <MenuItem mnemonicParsing="false" text="Reset Game" />
                  <MenuItem mnemonicParsing="false" text="Exit Game" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
